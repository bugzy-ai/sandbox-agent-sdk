{
  "name": "@bugzy-ai/sandbox-agent-sdk",
  "version": "0.1.1",
  "description": "Claude Agent SDK with Vercel Sandbox backing for serverless environments",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:simple": "tsx --env-file=.env.local scripts/test-simple.ts",
    "test:tools": "tsx --env-file=.env.local scripts/test-tools.ts",
    "test:comprehensive": "tsx --env-file=.env.local scripts/test-comprehensive.ts",
    "test:snapshots": "tsx --env-file=.env.local scripts/test-snapshots.ts",
    "test:oauth": "tsx --env-file=.env.local scripts/test-oauth.ts"
  },
  "dependencies": {
    "@vercel/sandbox": "^1.4.1",
    "@modelcontextprotocol/sdk": "^1.12.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^22.0.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.2.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "claude",
    "anthropic",
    "vercel",
    "sandbox",
    "serverless",
    "ai",
    "agent"
  ],
  "author": "Bugzy AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bugzy-ai/sandbox-agent-sdk.git"
  },
  "homepage": "https://github.com/bugzy-ai/sandbox-agent-sdk#readme",
  "bugs": {
    "url": "https://github.com/bugzy-ai/sandbox-agent-sdk/issues"
  }
}
